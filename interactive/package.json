{
  "name": "full-stack-boilerplate",
  "version": "0.0.1",
  "description": "A basic full-stack boilerplate (including express and mongoose)",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./dist",
    "build-client": "./init.sh && npm run clean && npm run copy && webpack --progress --mode development",
    "build-server": "babel server -s -D -d server-dist",
    "build": "npm run build-client && npm run build-server",
    "serve": "node server-dist"
  },
  "author": "Christian Londero",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "babel-loader": "^8.2.4",
    "babel-preset-env": "^1.7.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "body-parser": "^1.19.2",
    "clingojs": "^1.0.2",
    "express": "^4.17.3",
    "express-history-api-fallback": "^2.2.1",
    "express-jwt": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "page": "^1.11.6",
    "semantic-ui-css": "^2.4.1"
  }
}
